{
    "navigationFallback": {
      "rewrite": "index.html",
      "exclude": ["/static/images/*.{png,jpg,gif}", "/static/css/*"]
    },
    "mimeTypes": {
      ".json": "text/json"
    },
  
    "routes": [
      {
        "route": "/login",
        "serve": "/index.html",
        "statusCode": 200
      },
      {
        "route": "/register",
        "serve": "/index.html",
        "statusCode": 200
      },
      {
        "route": "/*",
        "serve": "/index.html",
        "statusCode": 200
      }
    ]
  }